#Program menentukan akar persamaan kuadrat
import math
a=int(input("Masukkan koefisien dari x kuadrat:"))
b=int(input("Masukkan koefisien dari x:"))
c=int(input("Masukkan nilai:"))
d=b**(2)-4*a*c
x1=(-b+math.sqrt(d))/2*a
x2=(-b-math.sqrt(d))/2*a
print("Jenis akar dari persamaan kuadrat tersebut adalah...")
if d<=0:
    print("akar imajiner")
elif d==0:
    print("akar kembar")
elif d>=0:
    print("akar real berlainan")
print("akar-akarnya adalah {0} dan {1}".format(x1, x2))
